{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:react/recommended", "prettier/react"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["react", "import"],
    "rules": {
        "no-console": "warn"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "parser": "babel-eslint"
}
